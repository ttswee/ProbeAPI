<?xml version="1.0"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ISpaceProbe" />
                <binding name="BasicHttpBinding_IFolderMaintenance" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://127.0.0.1:8080/ProbeAPI" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_ISpaceProbe" contract="probeAPI.ISpaceProbe"
                name="BasicHttpBinding_ISpaceProbe" />
            <endpoint address="http://127.0.0.1:8080/ProbeAPI" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IFolderMaintenance"
                contract="probeAPI.IFolderMaintenance" name="BasicHttpBinding_IFolderMaintenance" />
        </client>
    </system.serviceModel>
</configuration>
